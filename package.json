{
  "name": "endpointtester",
  "displayName": "endpointtester",
  "description": "Test simple endpoints within Code.",
  "version": "0.0.1",
  "repository": "github:jacobdejean/endpointtester",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [
    "onCommand:endpointtester.endpointTester",
    "onView:endpointtester.endpointTesterView"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "endpointtester.endpointTester",
        "title": "Endpoint Tester: Start"
      }
    ],
    "views": {
      "debug": [
        {
          "type": "webview",
          "id": "endpointtester.endpointTesterView",
          "name": "Endpoint Tester"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "data-uri-to-buffer": "^4.0.0",
    "fetch-blob": "^3.1.5",
    "formdata-polyfill": "^4.0.10",
    "marky": "^1.2.4",
    "node-domexception": "^1.0.0",
    "node-fetch-commonjs": "^3.1.1",
    "web-streams-polyfill": "^3.2.1"
  }
}

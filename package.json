{
  "name": "endpointtester",
	"displayName": "endpointtester",
	"description": "Test simple endpoints within Code.",
	"version": "0.0.1",
	"repository": "github:jacobdejean/endpointtester",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Testing"
	],
	"activationEvents": [
        "onCommand:endpointtester.endpointTester"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "endpointtester.endpointTester",
            "title": "Endpoint Tester: Start"

		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.67.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "14.x",
        "eslint": "^8.14.0",
        "glob": "^8.0.1",
        "mocha": "^9.2.2",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3"
    }

}
